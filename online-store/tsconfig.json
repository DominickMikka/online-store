{
  "compilerOptions": {
    "target": "es6",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "noImplicitAny": true,
    "moduleResolution": "node"
  },
  "include": ["./src/"],
  "exclude": ["node_modules", "webpack.config.js"]
}